public class Animal {
    String color="Blue";
    int x=12;
    public void eating(){
        System.out.println("animal is eating ");
    }
}
public class Dog extends Animal{
    int x=4;
    public void speak(){
    System.out.println(super.x);
    System.out.println(this.x);
    }
    public static void main(String[] args) {
        Dog d1=new Dog();
        d1.eating();
    }
}
